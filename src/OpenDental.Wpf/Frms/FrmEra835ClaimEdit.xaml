		<ui:Label Height="18" Margin="30,10,0,0" Text="Claim Fee" Width="88" VAlign="Center" HAlign="Right" />
		<ui:TextBox x:Name="textClaimFee" Height="20" Margin="122,10,0,0" Width="80" ReadOnly="True"/>
		<ui:Label Height="18" Margin="10,35,0,0" Text="Claim Fee Override" Width="108" VAlign="Center" HAlign="Right" />
		<ui:TextVDouble x:Name="textVDoubleClaimFeeOverride"  Height="20" Margin="122,35,0,0" Width="80" MinVal="0"/>
